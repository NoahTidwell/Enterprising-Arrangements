  <body>
    <!--Nav Bar at very top-->
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
      <div class="container-fluid">
        <a class="navbar-brand fs-3" href="#">Enterprising-Arrangements
</a>
        <button
          class="navbar-toggler"
          type="button"
          data-bs-toggle="collapse"
          data-bs-target="#navbarNavDropdown"
          aria-controls="navbarNavDropdow"
          aria-expanded="false"
          aria-label="Toggle navigation"
        >
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarNavDropdown">
          <ul class="navbar-nav">

                        <li class="nav-item m-3 fs-5">
              <a class="nav-link active" aria-current="page" href="/">Home</a>
            </li>

            <li class="nav-item m-3 fs-5">
              {{#if loggedIn}}
                <a
                  class="nav-link active"
                  aria-current="page"
                  href="/dashboard"
                >User Dashboard</a>

            <li class="nav-item">
              <button
                id="logout"
                type="button"
                class="btn btn-dark btn-lg rounded-pill mt-3 m-2"
              >Log-Out</button>
            </li>
              {{else}}
                <li class="nav-item">
                  <a class="nav-link" href="/login"><button
                      type="button"
                      class="btn btn-dark btn-lg rounded-pill mt-2"
                    >Sign-In</button></a>
              </li>
                        <li class="nav-item">
              <a class="nav-link" href="/signup">
                <button
                  type="button"
                  class="btn btn-dark btn-lg rounded-pill mt-2"
                >Sign-Up</button></a>
            </li>
              {{/if}}
          </ul>
        </div>
      </div>
    </nav>

    <!--End Navbar-->
    <!--Start of main section-->
    {{{body}}}
    <!--End of the Main Section-->

  
</div>
</div>
</div>
